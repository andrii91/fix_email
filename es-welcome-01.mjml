      <mj-section
        css-class="es-header"
        padding="35px 15px">
        <mj-column width="100%">
          <mj-image align="left" href="#" width="157px" src="https://andrii91.github.io/fix_email/assets/es-email-logo.png" />
          <mj-text align="left" color="#fff" font-size="36px" line-height="1.2" font-weight="700" text-transform="uppercase">Welcome to Elite seller</mj-text>
          <mj-spacer height="100px" />
        </mj-column>
      </mj-section>
      <!-- /es-header -->


      <!-- es-hi-john -->
      <mj-section padding="10px 40px 0 0">
