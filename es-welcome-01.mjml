<mjml>
  <mj-head>
    <mj-title>ES Seller</mj-title>
    <mj-preview>ES Seller</mj-preview>
    <mj-attributes>
      <mj-all font-family="'Helvetica Neue', Helvetica, Arial, sans-serif"></mj-all>
      <mj-text font-weight="400" font-size="16px" color="#000000" line-height="24px" font-family="'Helvetica Neue', Helvetica, Arial, sans-serif"></mj-text>
    </mj-attributes>
    <mj-style inline="inline">
      .body-section {
        -webkit-box-shadow: 0px 5px 50px #CAE7FF;
        -moz-box-shadow: 0px 5px 50px #CAE7FF;
        box-shadow: 0px 5px 50px #CAE7FF; }
    </mj-style>
    <mj-style inline="inline">
      .text-link { color: #5e6ebf }
    </mj-style>
    <mj-style inline="inline">
      .footer-link {
        color: #013073;
        text-decoration: none;
        margin: 0 10px; }
    </mj-style>
    <mj-style inline="inline">
      .body-text {
        color: #19153E; }
    </mj-style>
    <mj-style inline="inline">
      .blue-text {
        color: #00AAFF; }
    </mj-style>
    <mj-style inline="inline">
      .block-text {
        background: #FFFFFF;
        box-shadow: 10px 20px 40px rgba(0, 0, 0, 0.0957314);
        border-radius: 32px; }
    </mj-style>
    <mj-style inline="inline">
      .es-header {
        background-image: url("https://andrii91.github.io/fix_email/assets/es-email-header.jpg");
        background-size: cover;
        background-repeat: no-repeat;
        background-position: center bottom; }
    </mj-style>
    <mj-style inline="inline">
      .es-footer {
        background-image: url("https://andrii91.github.io/fix_email/assets/es-email-footer.jpg");
        background-size: cover;
        background-repeat: no-repeat;
        background-position: center top; }
    </mj-style>
    <mj-style inline="inline">
      .es-strategy {
        background-image: url("https://andrii91.github.io/fix_email/assets/es-email-strategy.jpg");
        background-size: cover;
        background-repeat: no-repeat;
        background-position: center; }
    </mj-style>

