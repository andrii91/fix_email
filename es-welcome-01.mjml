              <td align="center">
                <p style="font-size: 18px; line-height: 24px; color: #19153E; margin: 0 5px;">Setup your<br>Automations Bot</p>
                <img width="45px" src="https://andrii91.github.io/fix_email/assets/es-email-step-01.jpg" />
              </td>
            </tr>
          </mj-table>
        </mj-column>
      </mj-section>
      <!-- /es-plarforms -->


      <!-- es-elite -->
