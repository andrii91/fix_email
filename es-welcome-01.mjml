          </mj-button>
          <mj-image align="center" width="146px" padding="20px 0 0" src="https://andrii91.github.io/fix_email/assets/es-email-founders-02.png" />
        </mj-column>
      </mj-section>
      <!-- /es-elite -->


      <!-- es-footer -->
      <mj-section
        css-class="es-footer"
        padding="40px 40px 30px">

        <mj-column width="100%" padding="40px 0 0 0"></mj-column>

        <mj-column width="65%" padding="0">
          <mj-table padding="0">
            <tr>
